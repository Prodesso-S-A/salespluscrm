<div class="content-body">
    <div class="container-fluid mt-3">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Permisos</h4>
                <div class="button-icon">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered zero-configuration">
                            <thead>
                                <tr>
                                    <th>Permiso</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each permiso}}
                                <tr>
                                    <td>{{Nombre}}</td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>