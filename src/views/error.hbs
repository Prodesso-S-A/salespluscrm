{{#each errormsg}}    
    <div class="login-form-bg h-100">
        <div class="container h-100">
            <div class="row justify-content-center h-100">
                <div class="col-xl-6">
                    <div class="error-content">
                        <div class="card mb-0">
                            <div class="card-body text-center">
                                <h3 class="error-text text-primary">Algo Salio Mal :( Error Code: {{code}}</h3>
                                <h4 class="mt-4"><i class="text-danger">{{url}}</i></h4>
                                <p>{{Mensaje}}</p>
                                <p>{{stack}}</p>
                                <form class="mt-5 mb-5">
                                    <div class="text-center mb-4 mt-4"><a href="/dashboard" class="btn btn-primary">Go to Homepage</a>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}    